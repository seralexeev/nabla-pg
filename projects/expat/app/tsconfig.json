{
  "extends": "../../../tsconfig.json",
  "compilerOptions": {
    "jsx": "react-native",
    "outDir": "dist",
    "paths": {
      "@flstk/result": ["packages/result/src/index"],
      "@flstk/use-api": ["packages/use-api/src/index"],
      "@flstk/utils": ["packages/utils/src/index"],
      "@flstk/react-core": ["packages/react-core/src/index"],
      "@flstk/react-native": ["packages/react-native/src/index"],
      "@projects/expat/shared/*": ["projects/expat/shared/src/*"],
      "@projects/expat/app/*": ["projects/expat/app/src/*"],
      "@projects/expat/api/*": ["projects/expat/api/src/*"]
