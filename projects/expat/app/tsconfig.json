  },
  "references": [
    { "path": "../../../packages/result" },
    { "path": "../../../packages/use-api" },
    { "path": "../../../packages/utils" },
    { "path": "../../../packages/react-core" },
    { "path": "../../../packages/react-native" },
    { "path": "../shared" },
    { "path": "../api" }
