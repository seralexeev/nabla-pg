{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "debub intro",
      "type": "node",
      "request": "launch",
      "protocol": "inspector",
      "cwd": "${workspaceRoot}",
      "program": "${workspaceRoot}/node_modules/.bin/ts-node-dev",
      "args": [
        "--inspect",
        "--transpile-only",
        "-r",
        "tsconfig-paths/register",
        "${workspaceRoot}/packages/pg/src/intro.ts"
      ],
      "env": {},
      "restart": true,
      "console": "integratedTerminal",
      "outputCapture": "std"
    }
  ]
}
