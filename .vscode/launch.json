{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug expat-api",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceRoot}",
      "program": "${workspaceRoot}/node_modules/.bin/ts-node-dev",
      "args": ["--transpile-only", "-r", "tsconfig-paths/register", "${workspaceRoot}/projects/expat/api/src/index.ts"],
      "restart": true,
      "console": "integratedTerminal",
      "env": {
        "FLSTK_env": "dev",
        "TS_NODE_PROJECT": "${workspaceFolder}/projects/expat/api/tsconfig.json"
      }
    }
  ]
}
